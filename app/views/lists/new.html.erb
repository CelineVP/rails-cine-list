<div class="container py-4">
  <div class="row">
    <div class="col-10 col-md-8 offset-lg-2">
      <h1>Create a new list</h1>
      <div class="bg-white rounded p-5 shadow">
        <%= simple_form_for @list do |f| %>
          <%= f.input :name %>
          <%= f.submit class: 'btn btn-primary' %>
        <% end %>
      </div>
    </div>
  </div>
</div>


